/* pages/main.wxss */

.root {
    background: #f4f4f4;
    width: 100%;
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.contianer {
    width: 200px;
    height: 200px;
    background-repeat: no-repeat;
    background-size: 100%;
    background-image: url(data:image/png;base64,);
}

.point {
    width: 6rpx;
    height: 6rpx;
    background: black;
    position: absolute;
}

.stage {
    width: 260px;
    height: 260px;
}

.button_container{
    background: #f4f4f4;
    width: 100%;
    display: flex;
    flex: 2.5;
    flex-direction: column;
    align-items: center;
}

.bihua {
    min-height: 40px;
    width: 280px;
    height: 40px;
    background: white;
    background-repeat: no-repeat;
}

.icon {
    width: 26px;
    height: 26px;
}
audio{
    background-size: 100%;
    background-image: url(data:image/png;base64,)
}
.button {
    width: 120px;
    height: 36px;
    background-color: #2c5f97;
    border-radius: 4rpx;
    font-size: 38rpx;
    color: white;
    display: flex;
    justify-content: center;
    align-items: center;
}

text {
    font-size: 18px;
    font-weight: normal;
    color: #2c5f97;
}

.item_container {
    min-width: 260px;
    display: flex;
}

.searchbar-result {
    margin-top: 0;
    font-size: 14px;
}

.searchbar-result:before {
    display: none;
}

.weui-cell {
    padding: 12px 15px 12px 35px;
}
